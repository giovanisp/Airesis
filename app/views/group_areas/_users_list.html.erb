<% @group.participants.includes(:image).order('users.surname asc').each do |participant| %>
    <div class="members" id="mem<%= participant.id %>">
      <%= participant.user_image_tag 40 %>
      <b><%= link_to participant.fullname, participant %></b>
    </div>
<% end %>

<div style="clear:both;"></div>

