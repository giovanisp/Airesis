<%if @event.proposal_id && !@event.proposal_id.empty?%>
  $('#wait_date_header_container').html('<%= j render(partial: 'proposals/wait_date_header', locals: {proposal: @proposal})%>');
<%elsif params[:type] == 'election'%>
    window.location.reload();
<%else%>
$('#calendar').fullCalendar( 'refetchEvents' );
<%end%>
$('#create_event_dialog').foundation('reveal', 'close');
